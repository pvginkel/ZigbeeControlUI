    "/api/sse/callback": {
      "post": {
        "description": "This endpoint receives callbacks from the SSE Gateway when clients connect or disconnect. It extracts the request_id from the callback URL and calls SSEConnectionManager, which then notifies observers.\n\nReturns:     200 on success     401 if authentication fails (production only)     400 if payload invalid or request_id missing",
        "operationId": "post__api_sse_callback",
        "parameters": [],
        "responses": {},
        "summary": "Handle SSE Gateway connect/disconnect callbacks.",
        "tags": []
      }
    },
